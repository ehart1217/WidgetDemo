<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemeFullScreen" parent="Theme.AppCompat.NoActionBar">
        <item name="android:background">@color/black</item>
    </style>

    <style name="AppThemeFullScreenWhite" parent="Theme.AppCompat.NoActionBar">
        <item name="android:background">@color/white</item>
    </style>

    <!--SuperCheckbox的样式-->
    <style name="SuperCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_item_checked</item>
    </style>

</resources>
